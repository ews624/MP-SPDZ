#Python caller



a = Array(10,sint)
a.assign_all(0)
b = sint(2)


def check_column(column,length):
    answer = sint.MemValue(1)
    #column[0] = sint(1)
    @for_range_opt(length-1)
    def _(i):
        value = column[i]
        next = column[i+1]
        check = (next == value)
        answer.write(answer*check)

    print_ln("%s",answer.reveal())


def main():
    print_ln('Hello')
    check_column(a,10)

main()
